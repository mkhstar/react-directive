    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.32.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.3"
